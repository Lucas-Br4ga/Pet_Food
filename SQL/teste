CREATE DATABASE PetFood
USE PetFood
CREATE TABLE Usuarios (
Usuarios_id INT PRIMARY KEY AUTO_INCREMENT,
Nome VARCHAR(255) NOT NULL,
DataNasc DATE NOT NULL, 
cpf int NOT NULL,
ValeTransporte DECIMAL(10,2) NOT NULL,
ValeAlimetacao DECIMAL(10,2) NOT NULL,
Horario TIME(0) NOT NULL,
);
CREATE TABLE Produtos (
Produtos_id INT PRIMARY KEY AUTO_INCREMENT,
Nome VARCHAR(255) NOT NULL,
Preco DECIMAL(10,2) NOT NULL,
PrecoCusto DECIMAL(10,2) NOT NULL,
Quantidade INT NOT NULL,
);
